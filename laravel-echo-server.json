{
  "authHost": "http://localhost",
  "authEndpoint": "/broadcasting/auth",
  "clients": [
    {
      "appId": "my-app-id",
      "key": "my-app-key"
    }
  ],
  "database": "redis",
  "databaseConfig": {
    "redis": {
      "host": "redis",
      "port": "6379"
    }
  },
  "host": null,
  "port": "6001",
  "devMode": true,
  "protocol": "http",
  "sslCertPath": "",
  "sslKeyPath": "",
  "sslCertChainPath": "",
  "sslPassphrase": "",
  "subscribers": {
    "http": true,
    "redis": true
  },
  "apiOriginAllow": {
    "allowCors": true,
    "allowOrigin": "*",
    "allowMethods": "GET, POST",
    "allowHeaders": "Origin, Content-Type, X-Auth-Token, Authorization, X-Requested-With"
  }
}
